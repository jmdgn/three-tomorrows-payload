{
  "name": "my-payload-project",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:railway": "export RAILWAY_STATIC_URL=${RAILWAY_STATIC_URL:-https://three-tomorrows-payload-production.up.railway.app}; export NEXT_PUBLIC_SERVER_URL=${NEXT_PUBLIC_SERVER_URL:-https://three-tomorrows-payload-production.up.railway.app}; export PAYLOAD_PUBLIC_SERVER_URL=${PAYLOAD_PUBLIC_SERVER_URL:-https://three-tomorrows-payload-production.up.railway.app}; NODE_ENV=production next build && find .next -type f -name '*.js' -exec sed -i 's|http://localhost:3000|https://three-tomorrows-payload-production.up.railway.app|g' {} \\;",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "prepare": "echo 'Skipping type generation in production'",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "next start"
  },
  "dependencies": {
    "@lexical/code": "^0.28.0",
    "@lexical/link": "^0.28.0",
    "@lexical/list": "^0.28.0",
    "@lexical/react": "^0.28.0",
    "@lexical/selection": "^0.28.0",
    "@lexical/table": "^0.28.0",
    "@payloadcms/admin-bar": "latest",
    "@payloadcms/db-mongodb": "^3.35.0",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "^3.35.0",
    "@payloadcms/payload-cloud": "^3.35.0",
    "@payloadcms/plugin-cloud": "^3.0.2",
    "@payloadcms/plugin-form-builder": "^3.35.0",
    "@payloadcms/plugin-nested-docs": "^3.35.0",
    "@payloadcms/plugin-redirects": "^3.35.0",
    "@payloadcms/plugin-search": "^3.35.1",
    "@payloadcms/plugin-seo": "^3.35.0",
    "@payloadcms/richtext-lexical": "^3.35.0",
    "@payloadcms/storage-s3": "^3.35.1",
    "@payloadcms/ui": "^3.35.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@vercel/blob": "^1.0.0",
    "aws-sdk": "^2.1692.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "http-proxy-middleware": "^3.0.5",
    "isomorphic.js": "^0.2.5",
    "lexical": "^0.28.0",
    "lib0": "^0.2.104",
    "lucide-react": "^0.378.0",
    "monaco-editor": "^0.52.2",
    "mongodb": "^6.15.0",
    "next": "^15.3.1",
    "next-sitemap": "^4.2.3",
    "payload": "^3.35.1",
    "picomatch": "^4.0.2",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.175.0",
    "yjs": "^13.6.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.3",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "lucide-react": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  }
}
